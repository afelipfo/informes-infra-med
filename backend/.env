# Fichero: backend/.env

PROJECT_NAME="API de Generación de Informes - Infraestructura Medellín"
API_V1_STR="/api/v1"

# Base de datos PostgreSQL
DATABASE_URL=postgresql+asyncpg://postgres:password@informes-postgres:5432/informes_db

# Autenticación JWT
SECRET_KEY=your-super-secret-key-change-in-production-123456789
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis para cache
REDIS_URL=redis://informes-redis:6379

# Configuración de archivos
MAX_FILE_SIZE=10485760
ALLOWED_FILE_EXTENSIONS=[".csv",".xlsx",".xls"]

# Logging
LOG_LEVEL=INFO